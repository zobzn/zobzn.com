{
  "framework": "hugo",
  "buildCommand": "hugo version && hugo --gc --minify",
  "build": {
    "env": {
      "HUGO_VERSION": "0.133.1"
    }
  },
  "redirects": [
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/semyon-tokarev/",
      "statusCode": 302
    }
  ],
  "rewrites": [
    {
      "source": "/resume",
      "destination": "/download/cv-tokarev-web-dev-2020.pdf"
    },
    {
      "source": "/proxy.js",
      "destination": "https://simpleanalyticsexternal.com/proxy.js?hostname=zobzn.com&path=/simple"
    },
    {
      "source": "/auto-events.js",
      "destination": "https://scripts.simpleanalyticscdn.com/auto-events.js"
    },
    {
      "source": "/simple/:match*",
      "destination": "https://queue.simpleanalyticscdn.com/:match*"
    }
  ]
}