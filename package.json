    "raw-loader": "^4.0.0",
    "rehype-raw": "^4.0.1",
    "rehype-stringify": "^6.0.1",
    "remark-parse": "^7.0.1",
    "remark-rehype": "^5.0.0",
    "unified": "^8.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.8.18",
    "framer-motion": "^1.7.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 8",
    "> 1%"
