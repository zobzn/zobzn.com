{
  "name": "zobzn.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "lint": "eslint --fix -- .",
    "test": "jest --collect-coverage",
    "build": "next build",
    "start": "next start",
    "nodemon": "nodemon --exec \"npm run dev\"",
    "prettier": "prettier --write \"**/*.{js,jsx,vue,json,yml,yaml,scss,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add",
      "jest --bail --find-related-tests"
    ],
    "**/*.{yml,yaml,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.3.1",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.22",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gray-matter": "^4.0.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "next": "^9.1.6",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
