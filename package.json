{
  "version": "7.7.7",
  "name": "zobzn.com",
  "author": "Semyon Tokarev",
  "description": "My Notes And Tools Website",
  "homepage": "https://zobzn.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zobzn/zobzn.com/"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --fix -- ./src",
    "lint-staged": "lint-staged",
    "test": "jest --collect-coverage",
    "clean": "rimraf .next/ out/",
    "build": "next build",
    "start": "nodemon --exec \"next dev\"",
    "export": "npm run clean && next build && next export",
    "serve": "npx serve out -l 80",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{yml,yaml,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "lint-staged-bak": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add",
      "jest --bail --find-related-tests"
    ],
    "**/*.{yml,yaml,json,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-eslint": "^10.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gray-matter": "^4.0.3",
    "husky": "8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "next": "^13.4.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "typescript": "^5.1.3",
    "unified": "^10.1.2"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fontfaceobserver": "^2.3.0",
    "formik": "^2.4.1",
    "framer-motion": "^10.12.16",
    "front-matter": "^4.0.2",
    "glob": "^10.2.6",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-react": "^7.2.0"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 11",
    "> 1%"
  ],
  "volta": {
    "node": "18.16.0"
  }
}
